class FooBarCounter{

	foobar_count = 0
	total_count = 0

	count_foobars = {dir=Str|
		for file in dir.walk()[2] {
			if file.contains('foo') or file.contains('bar') {
				foobar_count+=1
				}else{
				foobar_count-=1
				}
		total_count+=1

		}
	}

}

